{
  "name": "redux-json-api-handlers",
  "version": "2.5.2",
  "description": "JSON-API entities and relationships handlers",
  "main": "./lib/index.js",
  "repository": "https://github.com/StartupCraft/json-api-handlers.git",
  "author": "George Viazemsky <code@and.graphics>",
  "license": "MIT",
  "scripts": {
    "build": "babel src/ -d lib/",
    "test": "jest",
    "test:cov": "jest --coverage --collectCoverageFrom=src/**/*.js",
    "lint": "eslint src __tests__",
    "lint:fix": "eslint src __tests__ --fix",
    "precommit": "lint-staged",
    "prepublish": "yarn test && yarn build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,css}": [
      "eslint src tests --fix",
      "prettier --config .prettierrc --write",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ]
  },
  "dependencies": {
    "lodash": "4.17.5",
    "redux-object": "^0.5.6",
    "reselect": "^3.0.1",
    "seamless-immutable": "7.1.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.2.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-latest": "6.24.1",
    "codecov": "3.0.0",
    "eslint": "4.17.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.43.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "flow-bin": "0.65.0",
    "husky": "0.14.3",
    "jest": "22.2.2",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2",
    "regenerator-runtime": "0.11.1"
  }
}
